generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// MAIN MODELS

model Category {
  id String @id @default(cuid())
  name String @unique
  slug String @unique
  description String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
